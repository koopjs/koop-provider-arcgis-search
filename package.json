{
  "name": "koop-provider-arcgis-search",
  "version": "1.0.0",
  "description": "Koop provider for ArcGIS Search",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node server.js",
    "debug": "NODE_ENV=test node server.js",
    "test": "standard && tape test/*.js | tap-spec",
    "lambda-create": "claudia create --name koop-provider-arcgis-search --handler lambda.handler --deploy-proxy-api --region us-east-1 --set-env KOOP_PORT=80,DEPLOY=export",
    "lambda-update": "claudia update --set-env KOOP_PORT=80,DEPLOY=export",
    "lambda-production": "claudia update --set-env KOOP_PORT=80,DEPLOY=export --version production"
  },
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "config": "^2.0.1",
    "koop": "^3.10.0",
    "request-promise": "^4.2.1"
  },
  "author": "Andrew Turner",
  "license": "Apache-2.0",
  "devDependencies": {
    "nock": "^9.0.2",
    "standard": "^10.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/koop-provider-arcgis-search.git"
  },
  "bugs": {
    "url": "https://github.com/koopjs/koop-provider-arcgis-search/issues"
  },
  "homepage": "https://github.com/koopjs/koop-provider-arcgis-search",
  "keywords": [
    "koop",
    "provider",
    "arcgis",
    "search"
  ]
}
